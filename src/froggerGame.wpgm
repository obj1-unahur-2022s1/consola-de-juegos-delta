import wollok.game.*
import frogger.* 
import nivel.*

program frogger {
	game.title("Frogger Game")
	game.width(20)
	game.height(15)
	
	
	pantallaInicio.configurar()
	//juego.configurar()
	//juego.iniciar()
	
	var musica = game.sound("remember-the-80s.mp3")
	musica.shouldLoop(true)
	musica.volume(0.1)
	game.schedule(500, {musica.play()})
	
	keyboard.p().onPressDo({musica.pause()})
	keyboard.o().onPressDo({musica.resume()})
	
	game.start()
	

}